[art_hunter]:af_base
GroupControlSection	= spawn_group
$spawn				= "artifacts\art_hunter"

$prefetch 			= 64
cform				= skeleton
class				= ARTEFACT

visual				= physics\art_sharik.ogf  
description			= art_hunter_description
inv_name			= art_hunter
inv_name_short			= art_hunter
inv_weight			= 0.5

inv_grid_width     = 1
inv_grid_height    = 1
inv_grid_x         = 36
inv_grid_y         = 34

slot 				 		= 1		; // primary
animation_slot		 		= 1		; type of the animation that will be used
belt = false

cost				= 1000

jump_height			= 0

particles			= anomaly2\artefact\artefact_gravi

lights_enabled = true	
trail_light_color = 0.6,0.7,0.5
trail_light_range = 0.5


