	<string id="ars-7.62x51_fmj">
		<text>Патроны 7.62x51мм НАТО</text>
	</string>
	<string id="ars-7.62x51_fmj_s">
		<text>7.62x51 FMJ</text>
	</string>
	<string id="ars-7.62x51_ap">
		<text>Патроны 7.62x51мм НАТО AP</text>
	</string>
	<string id="ars-7.62x51_ap_s">
